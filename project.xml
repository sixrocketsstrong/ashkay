<project>
  <pomVersion>3</pomVersion>
  <id>ashkay</id>
  <name>Ashkay Java Caching</name>
  <currentVersion>0.6</currentVersion>
  <organization>
    <name>Apache Software Foundation</name>
    <url>http://www.apache.org/</url>
    <logo>http://maven.apache.org/images/jakarta-logo-blue.gif</logo>
  </organization>
  <inceptionYear>2004</inceptionYear>
  <package>net.sf.ashkay</package>
  <logo>http://maven.apache.org/images/maven.jpg</logo>
  <description>
  	A different take on slicing the caching problem domain. Ashkay implements
	a strategy based caching model. Developers have obvious points of extension
	to this lightweight cache. The same cache can be turned off during 
	development with minimal code impacts.
  </description>
  <shortDescription>Strategy based Java caching.</shortDescription>
  <url>http://ashkay.sf.net</url>
  <issueTrackingUrl>http://nagoya.apache.org/scarab/servlet/scarab/</issueTrackingUrl>
  <siteAddress>ashkay.sf.net</siteAddress>
  <siteDirectory>/www/maven.apache.org/reference/plugins/examples/</siteDirectory>
  <distributionDirectory>/www/maven.apache.org/builds/</distributionDirectory>
  <repository>
    <connection>scm:cvs:pserver:anoncvs@cvs.apache.org:/home/cvspublic:maven-plugins/examples</connection>
    <url>http://cvs.apache.org/viewcvs/maven-plugins/examples/</url>
  </repository>
  <mailingLists/>
  <developers/>
  <dependencies>
  	<dependency>
		<id>log4j</id>
		<version>1.2.8</version>
	</dependency>
	<dependency>
		<id>concurrent</id>
		<version>1.3.4</version>
	</dependency>
  </dependencies>
  <build>
    <nagEmailAddress>bangroot@users.sf.net</nagEmailAddress>
    <sourceDirectory>src/java</sourceDirectory>
    <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
    </unitTest>
    <resources>
      <resource>
        <directory>src/conf</directory>
        <includes>
          <include>*.properties</include>
        </includes>
      </resource>
    </resources>
  </build>
</project>
